{
  "name": "dataqueue-monorepo",
  "private": true,
  "devDependencies": {
    "pnpm": "^9.0.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.4"
  },
  "scripts": {
    "dev": "turbo run dev cron --parallel --filter demo --filter \"@nicnocquee/dataqueue\"",
    "dev:docs": "turbo run dev --filter docs",
    "dev:demo": "turbo run dev --filter demo",
    "dev:website": "turbo run dev --filter website",
    "dev:dataqueue": "turbo run dev --filter \"@nicnocquee/dataqueue\"",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write .",
    "check-format": "prettier --check ."
  },
  "packageManager": "pnpm@9.0.0"
}
